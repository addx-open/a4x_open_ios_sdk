# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'
source 'http://192.168.31.7:7990/scm/ios/moudlesspecs.git'
#source 'https://github.com/CocoaPods/Specs.git'

target 'A4xDemo_OC' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # Demo
  pod 'Masonry'
  
  # Pods for Base SDK
  # web request
  pod 'Moya', '~> 13.0'
  # Logger
  pod 'XCGLogger', '~> 7.0.1'
  # Router
  pod 'URLNavigator', '~> 2.1.0'
  # baseUI
  pod 'AutoInch', '~> 1.3.1'
  pod 'SnapKit' , '~> 5.0.0'
  pod 'MJRefresh' , '~> 3.2'
  # base extension
  pod 'A4xBaseExtensionKit', :path => 'A4xBaseExtensionKit'
  # baseUI
  pod 'A4xYogaKit', :path => 'A4xYogaKit'
  
  # Pods for Bind SDK
  # Bind UI
  pod 'SCIndexView', '~>2.2.4'
  # 'lottie-ios' for BindSDK && Live SDK
  pod 'lottie-ios', :path => 'lottie-ios'
  pod 'YYWebImage', '~>1.0.5'
  pod 'ZKDownload', :path => 'ZKDownload'

  # Pods for DeviceSetting SDK
  # deviceSetting UI
  pod 'YYWebImage' , '~> 1.0.5'
  pod 'FSCalendar', '~> 2.8'
  pod 'SCPageControl', :path => 'SCPageControl'
  
  # Pods for Live SDK
  # Live UI
  pod 'SDCAlertView'
  pod 'IQKeyboardManager'
  # Live
  pod 'A4xVideoPushManager', :path => 'A4xVideoPushManager'
  pod 'A4xWebRtcFramework', :path => 'A4xWebRtcFramework'
  pod 'Starscream'
  pod 'LFLiveKit', :path => 'LFLiveKit'

end
